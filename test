from io import BytesIO
from PIL import Image
import requests

import os

import configparser

if __name__ == '__main__':



   # 创建ConfigParser对象
   config = configparser.ConfigParser()

   # 读取INI文件
   config.read('config.ini')

   # 获取INI文件中所有的section
   sections = config.get("card","background")

   # 打印所有的section

   print(sections)